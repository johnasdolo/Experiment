 int main ()
 {
double total; 
float Mass;
float Density;
float volume;
	
 

	//Calculated Volume 
	cout << "Calculated Volume: " << endl;

	cout << " " << endl;

	cout << "Input Mass (grams) : " << endl;
	cin >> Mass;

	cout << " " << endl;

	cout << "Input Density (cubic centimeters) : " << endl; 
	cin >> Density;

	cout << " " << endl; 

